                        <li class="menu__item menu__dropdown busca">
                            <a class="menu__item-btn dropdown-toggle busca_botao" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-search" aria-hidden="true"></i><span class="sr-only">Botão Pesquisar</span></a>
                            <ul
                                class="dropdown-menu sub-menu">
                                <li class="sub-menu__item">
                                    <div class="busca__conteudo">
                                        <div class="busca__linha">
                                            <form class="formulario" action="https://www.tse.jus.br/@@search-es">
                                                <fieldset>
                                                    <legend class="sr-only">Pesquisa principal do portal</legend>
                                                    <div class="formulario__form-group">
                                                        <label class="sr-only" for="buscaPortal">Pesquisar</label><input type="text" class="formulario__form-control" name="searchable_text" id="buscaPortal" aria-describedby="ajudaBusca" placeholder="Digite aqui sua pesquisa"
                                                            autocomplete="off"><input type="hidden" name="rotulo_noticia" id="rotulo_noticia">
                                                        <div class="opcao-pesquisa exibir_secao">
                                                            <input type="checkbox" id="opcao-pesquisa" name="path_section" aria-label="Buscar somente em..." title="Buscar somente em RESOLUÇÃO Nº 23.610, DE 18 DE DEZEMBRO DE 2019." value="/portalje/tse/legislacao/compilada/res/2019/resolucao-no-23-610-de-18-de-dezembro-de-2019">
                                                            <label
                                                                for="opcao-pesquisa">Apenas nesta seção</label><input type="hidden" name="search_path_section" id="search_path_section" value="">
                                                        </div>
                                                        <script language="JavaScript">
                                                            $(document).ready(function() {
                                                                $("#opcao-pesquisa").change(function() {
                                                                    if (this.checked) {
                                                                        $('#search_path_section').val('section');
                                                                    } else {
                                                                        $('#search_path_section').removeAttr('value');
                                                                    }
                                                                });
                                                            });
                                                        </script><small id="ajudaBusca" class="formulario__form-text">pressione <strong>enter</strong> para pesquisar</small><button type="button" class="busca_fechar" aria-label="Fechar"><span aria-hidden="true">×</span></button>
                                                    </div>
                                                    <button type="submit" class="sr-only">Enviar</button>
                                                </fieldset>
                                            </form>
                                        </div>
                                    </div>
                                </li>
                    </ul>
                    </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    <!-- end:menu -->
